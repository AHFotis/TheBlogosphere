<div class="home">
  <h1 style="color:rgba(94, 39, 39, 0.743); font-weight:bold">Dashboard</h1>
  </div>
<div class="family">
{{#if loggedIn}}
{{#each blogs}}
<a href="/api/dash/{{id}}" class="blogLink">
